  "examples": [
    {
      "smalltalk": "'hello' at: 2",
      "result": "101",
      "explanation": "Code point for 'e'"
    },
    {
      "smalltalk": "'ABC' at: 1",
      "result": "65",
      "explanation": "Code point for 'A'"
